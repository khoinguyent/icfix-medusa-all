{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build:admin",
  "outputDirectory": "admin",
  "framework": null,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
